<div class="form">
	<%= form_for(@feed) do |f| %>
	  <%= render 'shared/error_messages', object: f.object %>
	  <%= f.label :name %>
	  <%= f.text_field :name %>

	  <%= f.label :description %>
	  <%= f.text_area :description %>

	  <%= f.hidden_field :section_id, :value => @section.id %>
	  <%= f.hidden_field :section, :value => @section %>

	  <%= f.submit "Create", class: "btn btn-large btn-primary" %>
	<% end %>
</div>